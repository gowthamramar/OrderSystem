<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/ll_item_grocery_search"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">
	<com.techware.clickkart.widgets.CustomTextView
		android:layout_marginTop="@dimen/_10sdp"
		android:id="@+id/txt_itemGrocery"
		android:textStyle="bold"
		android:text="@string/abc"
		android:textAppearance="@style/TextAppearance.AppCompat.Medium"
		android:textColor="@color/location_item"
		android:layout_marginLeft="@dimen/_20sdp"
		android:layout_marginStart="@dimen/_20sdp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"/>
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
	<com.techware.clickkart.widgets.CustomTextView
	
		android:text="Shop:"
		android:textAppearance="@style/TextAppearance.AppCompat.Small"
		android:textStyle="bold"
		android:textColor="@color/location_item"
		android:layout_marginLeft="@dimen/_20sdp"
		android:layout_marginStart="@dimen/_20sdp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>
		<com.techware.clickkart.widgets.CustomTextView
			android:id="@+id/txt_itemGroceryStore"
			android:text="Shop:"
			android:textAppearance="@style/TextAppearance.AppCompat.Small"
			android:textColor="@color/location_item"
			android:layout_marginLeft="@dimen/_3sdp"
			android:layout_marginStart="@dimen/_3sdp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
	android:orientation="horizontal"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">
	<com.techware.clickkart.widgets.CustomTextView
		
		android:text="@string/rs_grocery"
		android:textAppearance="@style/TextAppearance.AppCompat.Small"
		android:textColor="@color/location_item"
		android:layout_marginLeft="@dimen/_20sdp"
		android:layout_marginStart="@dimen/_20sdp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>
	<com.techware.clickkart.widgets.CustomTextView
		android:id="@+id/item_grocery_price"
		android:textAppearance="@style/TextAppearance.AppCompat.Small"
		android:textColor="@color/location_item"
		android:layout_marginLeft="@dimen/_3sdp"
		android:layout_marginStart="@dimen/_3sdp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"/>
</LinearLayout>
	<View
		android:layout_marginTop="@dimen/_5sdp"
		android:background="#bbbbbb

"
		android:layout_width="match_parent"
		android:layout_height="@dimen/_1sdp"/>
</LinearLayout>
